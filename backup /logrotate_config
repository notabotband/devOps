/opt/log/sausage-store-frontend.log {
  rotate 3
  create
  hourly
  delaycompress
  missingok
  size 10M
  postrotate
    date >> /opt/student_rotate.log
  endscript
}

/opt/log/sausage-store-backend.log {
  rotate 3
  create
  hourly
  delaycompress
  missingok
  size 10M
  postrotate
    date >> /opt/student_rotate.log
  endscript
}